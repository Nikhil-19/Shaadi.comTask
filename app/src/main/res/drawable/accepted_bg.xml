<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <solid android:color="@android:color/white" />
            <stroke
                android:width="1dp"
                android:color="#4cc97b" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:color="@android:color/transparent"
                android:width="4.5dp"/>
            <gradient
                android:angle="45"
                android:endColor="#1fc1b2"
                android:startColor="#4cc97b" />
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
    <item
        android:left="5dp"
        android:top="5dp"
        android:bottom="5dp"
        android:right="5dp"
        android:gravity="center"
        android:drawable="@drawable/ic_baseline_check_white">

    </item>
</layer-list>